generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id               String     @id @default(uuid())
  phoneNumber      String     @unique
  notificationTime String
  responses        Response[]
}

model Response {
  id        String   @id @default(uuid())
  userId    String
  emotion   String
  createdAt DateTime @default(now())
  user      User     @relation(fields: [userId], references: [id])
}

